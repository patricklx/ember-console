{
  "name": "ember-ink",
  "version": "0.1.0",
  "description": "Terminal UI library for Ember.js - inspired by Ink",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ember",
    "terminal",
    "cli",
    "tui",
    "console",
    "ink"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "chalk": "^5.3.0",
    "yoga-layout": "^3.1.0"
  },
  "devDependencies": {
    "@glint/core": "^1.4.0",
    "@glint/environment-ember-loose": "^1.4.0",
    "@glint/environment-ember-template-imports": "^1.4.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "rollup": "^4.9.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2"
  }
